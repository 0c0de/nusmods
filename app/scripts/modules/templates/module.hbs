<div class="row corspedia">
  <div class="col-md-3">
    <h1 class="module-code">{{ModuleCode}}</h1>
    <h3 class="module-title">{{ModuleTitle}}</h3>
    <hr>
    <dl>
      <dd class="module-desc {{#unless ShortModuleDescription}} module-desc-more {{/unless}}">
        <span class="desc-less">{{ShortModuleDescription}}...
          <div class="desc-full">( <a href="#" class="show-full-desc">See full description</a> )</div>
        </span>
        <span class="desc-more">{{ModuleDescription}}</span>
      </dd>
      <dt class="highlight">Credits</dt>
      <dd>{{ModuleCredit}}</dd>
      {{#if Prerequisite}}
        <dt class="highlight">Pre-requisite(s)</dt>
        <dd>{{{ParsedPrerequisite}}}</dd>
      {{/if}}
      {{#if Preclusion}}
        <dt class="highlight">Preclusion(s)</dt>
        <dd>{{{ParsedPreclusion}}}</dd>
      {{/if}}
      <dt class="highlight">Department</dt>
      <dd>{{Department}}</dd>
      <dt class="highlight">Exam Date</dt>
      {{#if ExamDate}}
        <dd>{{ExamDate}}</dd>
      {{else}}
        <dd>No Exam</dd>
      {{/if}}
      {{#if Workload}}
        <dt class="highlight">Weekly Workload</dt>
        <dl class="workload-components">
          {{#if WorkloadComponents.lectureHours}}
            <dd>Lecture: {{WorkloadComponents.lectureHours}} hrs</dd>
          {{/if}}
          {{#if WorkloadComponents.tutorialHours}}
            <dd>Tutorial: {{WorkloadComponents.tutorialHours}} hrs</dd>
          {{/if}}
          {{#if WorkloadComponents.labHours}}
            <dd>Lab: {{WorkloadComponents.labHours}} hrs</dd>
          {{/if}}
          {{#if WorkloadComponents.projectHours}}
            <dd>Project: {{WorkloadComponents.projectHours}} hrs</dd>
          {{/if}}
          {{#if WorkloadComponents.preparationHours}}
            <dd>Preparation: {{WorkloadComponents.preparationHours}} hrs</dd>
          {{/if}}
        </dl>
      {{/if}}
    </dl>
  </div>
