{{#each stats}}
  <h3 class="highlight thin">{{Semester}}</h3>
  <table class="table table-striped">
    <tbody>
      <tr>
        <th>Faculty</th>
        <th>Group</th>
        <th>Round</th>
        <th>Quota</th>
        <th>Bidders</th>
        <th>Lowest Bid</th>
        <th>Lowest Succ Bid</th>
        <th>Highest Bid</th>
        <th>Student Acct Type</th>
      </tr>
      {{#each BiddingStats}}
        <tr>
          <td>{{Faculty}}</td>
          <td>{{Group}}</td>
          <td>{{Round}}</td>
          <td>{{Quota}}</td>
          <td>{{Bidders}}</td>
          <td>{{LowestBid}}</td>
          <td>{{LowestSuccessfulBid}}</td>
          <td>{{HighestBid}}</td>
          <td>{{StudentAcctType}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
{{/each}}